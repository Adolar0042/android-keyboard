    <string name="payment_screen_set_reminder_setter_6_months">za 6 miesięcy</string>
    <string name="payment_screen_set_reminder_setter_next_century">w przyszłym stuleciu</string>
    <string name="payment_screen_set_reminder_setter_confirm">Potwierdź przypomnienie</string>
