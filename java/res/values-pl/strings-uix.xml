    <string name="payment_screen_need_help_subtitle">Potrzebujesz pomocy? Odwiedź naszą stronę internetową</string>

    <string name="payment_screen_pay_via_google">Zapłać przez Google Play (<xliff:g name="price" example="~$2.99">%s</xliff:g> + podatek)</string>
    <string name="payment_screen_pay_via_futopay">Zapłać (<xliff:g name="price" example="~$2.99">%s</xliff:g> + podatek)</string>
